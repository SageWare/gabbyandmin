          "_customHTML": "<section data-bs-version=\"5.1\" class=\"form1 expertm5\" group=\"Forms\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showMainSubtitle\">\n        <input type=\"checkbox\" title=\"Info Card\" name=\"showCard\" checked>\n        <header condition=\"showCard\">Card</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" condition=\"showCard\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" condition=\"showCard\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Social Title\" name=\"showSocialTitle\" condition=\"showSocial && showCard\" checked>\n        <input type=\"color\" title=\"Social Color\" name=\"socialColor\" value=\"#000000\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Social BG Color 1\" name=\"socialBgColor1\" value=\"#bbbbbb\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Social BG Color 2\" name=\"socialBgColor2\" value=\"#d8d8d8\" condition=\"showSocial && showCard\">\n        <input type=\"checkbox\" name=\"socialShadow\" title=\"Social Shadow\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"socialShadow1\" value=\"#000000\" condition=\"socialShadow && showSocial && showCard\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"socialShadow2\" value=\"#92b1c0\" condition=\"socialShadow && showSocial && showCard\">\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"6\" step=\"1\" value=\"4\" condition=\"showSocial && showCard\">\n        <header condition=\"showCard\">Card Style</header>\n        <input type=\"checkbox\" title=\"Card BG\" name=\"showCardBg\" condition=\"showCard\" checked>\n        <input type=\"color\" title=\"Card BG Color\" name=\"cardBg\" value=\"#ffffff\" condition=\"showCard && showCardBg\">\n        <input type=\"checkbox\" name=\"cardShadow\" title=\"Card Shadow\" condition=\"showCard\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"shadowColor1\" value=\"#c1c1c1\" condition=\"cardShadow && showCard\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"shadowColor2\" value=\"#c1c1c1\" condition=\"cardShadow && showCard\">\n        <header>Form Style</header>\n        <input type=\"checkbox\" title=\"Form BG\" name=\"showFormBg\">\n        <input type=\"color\" title=\"Form BG Color\" name=\"formBg\" value=\"#090f10\" condition=\"showFormBg\">\n        <input type=\"checkbox\" name=\"formShadow\" title=\"Form Shadow\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"formShadowColor1\" value=\"#879a9f\" condition=\"formShadow\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"formShadowColor2\" value=\"#bbbbbb\" condition=\"formShadow\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background4.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#212428\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"bg.type!='color' && overlay\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\" mbr-if=\"showMainTitle || showMainSubtitle\">\n                <div class=\"mbr-section-head\">\n                    <h3 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showMainTitle\" data-app-selector=\".mbr-section-title\"><b>CONTACT</b></h3>\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showMainSubtitle\" data-app-selector=\".mbr-section-subtitle\"></h5>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"row {{contentAlign}}\">\n            <div class=\"col-12 col-lg-4 col-info-card\" mbr-if=\"showCard\">\n                <div class=\"item-card\">\n                    <div class=\"item-wrap\">\n                        <div class=\"card-bg\"></div>\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/bookcase-with-logo.jpg\" alt=\"Delio Enterprises Lobby\">\n                        </div>\n                        <div class=\"card-box\" mbr-if=\"showTitle || showSubtitle || showText || showContacts || showSocial\">\n                            <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                               <b>Gabby &amp; Min</b>&nbsp;is a venture of <a href=\"https://delioenterprises.com\" class=\"text-secondary\" target=\"_blank\"><b>Delio Enterprises</b></a>. Submissions via this contact form are sent to Delio Enterprises. You should expect a response from contact@delioenterprises.com and be sure to check your spam folder.<br></h5>\n                            <h6 class=\"card-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".card-subtitle\"></h6>\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"><br><b>Hours&nbsp;</b>-<b>&nbsp;Eastern Standard Time (EST):</b><br>Monday: 9am - 5pm\n<br>Tuesday: 9am - 5pm\n<br>Wednesday: 9am - 5pm\n<br>Thursday: 9am - 5pm\n<br>Friday: 9am - 3pm\n<br><br>Closed weekends and all major holidays.<br></p>\n                            <p class=\"card-contacts mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showContacts\" data-app-selector=\".card-contacts\"></p>\n                            <div class=\"social-container\" mbr-if=\"showSocial\">\n                                <p class=\"social-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSocialTitle\" data-app-selector=\".social-title\"><b>\n                                    CONNECT WITH GABBY &amp; MIN:</b></p>\n                                <div class=\"social-list\">\n                                    <div class=\"soc-item\">\n                                        <a href=\"https://www.facebook.com/gabbyandmin\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-facebook socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                                        <a href=\"https://www.instagram.com/gabbyandmin/\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-instagram socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                                        <a href=\"https://twitter.com/gabbyandmin\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-twitter socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                                        <a href=\"https://www.linkedin.com/company/gabbyandmin/\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-linkedin socicon\"></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>4\">\n                                        <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>5\">\n                                        <span mbr-icon class=\"socicon-vimeo socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-lg-8 col-form\">\n                <div class=\"form-main-container\">\n                    <div class=\"form-wrap\">\n                        <div mbr-form class=\"mbr-form\">\n<!--Formbuilder Form-->\n<form action=\"contact@delioenterprises.com\" name=\"Contact Delio Enterprises\" method=\"POST\" class=\"mbr-form form-with-styler mx-auto\">\n<div class=\"row\">\n<div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thank you for contacting Gabby &amp;amp;amp;amp; Min at Delio Enterprises. Your inquiry will be processed and we will contact you as soon as possible.</div>\n<div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\"> Failed to send, please check back later. </div>\n</div>\n<div class=\"dragArea row\">\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\" data-for=\"Department: \">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Department: \" class=\"form-control-label mbr-fonts-style\">Department* </label>\n<select name=\"Department: \" data-form-field=\"Department: \" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control form-select\">\n<option value=\"ATTN: Gabby & Min - General Inquiry\">ATTN: Gabby &amp; Min - General Inquiry</option>\n<option value=\"ATTN: Gabby & Min - Shop Assistance\">ATTN: Gabby &amp; Min - Shop Assistance</option>\n<option value=\"ATTN: Gabby & Min - Literary Review Inquiry\">ATTN: Gabby &amp; Min - Literary Review Inquiry</option>\n<option value=\"ATTN: Gabby & Min - Distribution\">ATTN: Gabby &amp; Min - Distribution</option>\n</select>\n</div>\n<div class=\"col-lg-6 col-md-12 col-sm-12 form-group mb-3\" data-for=\"Email: \">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Email: \" class=\"form-control-label mbr-fonts-style\">Email*</label>\n<input type=\"email\" name=\"Email: \" placeholder=\"test@email.com\" data-form-field=\"Email: \" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control display-7\" value>\n</div>\n<div data-for=\"Phone Number: \" class=\"col-lg-6 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Phone Number: \" class=\"form-control-label mbr-fonts-style\">Phone Number</label>\n<input type=\"text\" name=\"Phone Number: \" placeholder=\"000-000-0000\" data-form-field=\"Phone Number: \" mbr-theme-style=\"display-7\" class=\"form-control display-7\" value>\n</div>\n<div data-for=\"Website or Social Media Link: \" class=\"col-lg-6 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Website or Social Media Link: \" class=\"form-control-label mbr-fonts-style\">Website or Social Media Link</label>\n<input type=\"url\" name=\"Website or Social Media Link: \" placeholder=\"https://\" data-form-field=\"Website or Social Media Link: \" mbr-theme-style=\"display-7\" class=\"form-control\" pattern=\"https://.*\" value=\"https://\">\n</div>\n<div data-for=\"Message: \" class=\"col-lg-12 col-md-12 col-sm-12 form-group mb-3\">\n<label mbr-text mbr-theme-style=\"display-7\" for=\"Message: \" class=\"form-control-label mbr-fonts-style\">Message*</label>\n<textarea name=\"Message: \" data-form-field=\"Message: \" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-control display-7\"></textarea>\n</div>\n<div class=\"col-lg-12 col-md-12 col-sm-12 form-group\">\n<div class=\"form-control-label\">\n</div>\n<div data-for=\"By continuing, you agree to our Terms of Service and Privacy Policy.\" class=\"form-check form-check-inline ms-2\">\n<input type=\"checkbox\" value=\"Yes\" name=\"By continuing, you agree to our Terms of Service and Privacy Policy.\" data-form-field=\"By continuing, you agree to our Terms of Service and Privacy Policy.\" mbr-theme-style=\"display-7\" required=\"required\" class=\"form-check-input\">\n<label mbr-text for=\"By continuing, you agree to our Terms of Service and Privacy Policy.\" mbr-theme-style=\"display-7\" class=\"form-check-label\">By continuing, you agree to our <a href=\"https://gabbyandmin.com/terms\" class=\"text-primary\" target=\"_blank\">Terms of Service</a> and <a href=\"https://gabbyandmin.com/privacy-policy\" class=\"text-primary\" target=\"_blank\">Privacy Policy</a>.</label>\n</div>\n</div>\n<div mbr-buttons=\"true\" mbr-theme-style=\"display-7\" data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" class=\"col-12 align-center mbr-section-btn custom-section-btn\">\n<a data-toolbar=\"-mbrBtnMove,-mbrLink,-mbrBtnRemove,-mbrBtnAdd\" type=\"submit\" class=\"w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 w-100 btn btn-black\">SEND MESSAGE</a>\n</div>\n</div>\n</form><!--Formbuilder Form-->\n</div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
